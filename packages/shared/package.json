{
  "name": "@aws-ai/shared",
  "version": "1.0.0",
  "description": "Shared utilities and types for AWS AI project",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "build": "tsc",
    "test": "vitest",
